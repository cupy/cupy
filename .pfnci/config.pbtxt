configs {
  key: "cupy.win.cuda102"
  value {
    requirement {
      cpu: 8
      memory: 24
      disk: 10
      gpu: 2
      image: "windows"
    }
    time_limit {
      seconds: 36000
    }
    checkout_strategy {
      include_dot_git: true
    }
    environment_variables { key: "GPU" value: "2" }
    command: ".pfnci\\windows\\run.bat 10.2 3.7 test"
  }
}

