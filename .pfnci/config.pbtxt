###
### Linux (CUDA) CI Projects
###

# CUDA 11.2 | Linux
configs {
  key: "cupy.linux.cuda112"
  value {
    requirement {
      cpu: 8
      memory: 50
      disk: 10
      gpu: 1
    }
    time_limit: {
      seconds: 21600
    }
    environment_variables { key: "GPU" value: "1" }
    command: ".pfnci/linux/main-flexci.sh cuda112"
  }
}

