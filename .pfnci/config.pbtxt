###
### Linux (CUDA) CI Projects
###

# Project to build Docker images
configs {
  key: "cupy.prep-linux"
  value {
    requirement {
      cpu: 12
      memory: 64
      disk: 10
      image: "docker"
    }
    time_limit: {
      seconds: 3600
    }
    command: ".pfnci/linux/main-flexci-prep.sh"
  }
}

# CUDA 10.2 | Linux
configs {
  key: "cupy.linux.cuda102"
  value {
    requirement {
      cpu: 8
      memory: 50
      disk: 10
      gpu: 1
    }
    time_limit: {
      seconds: 21600
    }
    environment_variables { key: "GPU" value: "1" }
    command: ".pfnci/linux/main-flexci.sh cuda102"
  }
}

